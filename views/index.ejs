<html lang="en">
	<%- include('./partials/header.ejs') %>
	<body>
		<%- include('./partials/nav.ejs') %>
		<div class="blog content">
			<h2>All Users</h2>
			<% if (users.length > 0) {%>
				<% users.forEach(user => {%>
					<a class="single" href="">
						<h3 class="title"><%= user.name %></h3>
						<p class="snippet"><%= user.email %></p>
					</a>
				<%}) %>
			<%} else { %>
				<p>There is no user to display ...</p>
			<% } %>
		</div>

		<%- include('./partials/footer.ejs') %>
	</body>
</html>